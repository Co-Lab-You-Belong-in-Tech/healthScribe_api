<div id="<%= dom_id food %>">
  <p>
    <strong>Name:</strong>
    <%= food.name %>
  </p>

  <p>
    <strong>Comment:</strong>
    <%= food.comment %>
  </p>

  <p>
    <strong>Time:</strong>
    <%= food.time %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= food.date %>
  </p>

  <p>
    <strong>Symptom:</strong>
    <%= food.symptom_id %>
  </p>

</div>
